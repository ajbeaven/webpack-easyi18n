{
    "name": "webpack-easyi18n",
    "version": "0.3.8",
    "description": "Go from gettext catalog (.po files) to embeded localization in your Webpack bundles",
    "engines": {
        "node": ">=4.3.0 <5.0.0 || >=5.10"
    },
    "files": [
        "src/"
    ],
    "main": "src/index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "clean:example": "rimraf example\\dist example\\locale\\webpack-easyi18n-temp",
        "build:example": "npm run clean:example && webpack -c example\\webpack.config.js"
    },
    "dependencies": {
        "i18next-conv": "^4.0.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ajbeaven/webpack-easyi18n.git"
    },
    "keywords": [
        "webpack",
        "i18n"
    ],
    "author": "Andrew Beaven",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ajbeaven/webpack-easyi18n/issues"
    },
    "homepage": "https://github.com/ajbeaven/webpack-easyi18n#readme",
    "devDependencies": {
        "html-webpack-plugin": "^5.3.1",
        "rimraf": "^3.0.2",
        "webpack": "^5.32.0",
        "webpack-cli": "^4.6.0"
    }
}
